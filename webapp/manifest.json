{
	"_version": "1.8.0",
	"sap.app": {
		"id": "com.ui5.SAPUI5_Session",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		},
		"dataSources": {
			"GWSAMPLE_BASIC": {
				"uri": "/ES5/sap/opu/odata/iwbep/GWSAMPLE_BASIC/",
				"type": "OData",
				"settings": {
					"localUri": "localService/GWSAMPLE_BASIC/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "com.ui5.SAPUI5_Session.view.App",
			"id": "mAppId",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "com.ui5.SAPUI5_Session.i18n.i18n"
				}
			},
			"Product": {
				"type": "sap.ui.model.json.JSONModel",
				"settings": {},
				"uri": "model/Products.json",
				"preload": false
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "TwoWay",
					"defaultCountMode": "InlineRepeat",
					"useBatch": true
				},
				"dataSource": "GWSAMPLE_BASIC",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "com.ui5.SAPUI5_Session.view",
				"controlAggregation": "pages",
				"controlId": "mAppId",
				"clearControlAggregation": false,
				"viewLevel": 1,
				"bypassed": {
					"target": [
						"PageNotFound"
					]
				}
			},
			"routes": [
				{
					"name": "MainView",
					"pattern": "",
					"target": [
						"MainView"
					]
				},
				{
					"name": "SecondView",
					"pattern": "SecondView/(:Obj:)",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"SecondView"
					]
				},
				{
					"name": "AddProducts",
					"pattern": "AddProducts",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"AddProducts"
					]
				},
				{
					"name": "ProductDisplayAndEdit",
					"pattern": "ProductEdit/{ProductPath}",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"ProductDisplayAndEdit"
					]
				}
			],
			"targets": {
				"MainView": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "MainView",
					"viewId": "MainView",
					"viewLevel": 1,
					"controlAggregation": "pages",
					"controlId": "mAppId"
				},
				"SecondView": {
					"viewType": "XML",
					"viewName": "SecondView",
					"viewId": "SecondView",
					"viewLevel": 2,
					"controlAggregation": "pages",
					"controlId": "mAppId"
				},
				"AddProducts": {
					"viewType": "XML",
					"viewName": "AddProducts",
					"viewId": "AddProducts",
					"viewLevel": 1,
					"controlAggregation": "pages",
					"controlId": "mAppId"
				},
				"ProductDisplayAndEdit": {
					"viewType": "XML",
					"viewName": "ProductDisplayAndEdit",
					"controlId": "mAppId",
					"viewId": "ProductEdit",
					"viewLevel": 4,
					"controlAggregation": "pages"
				},
				"PageNotFound": {
					"viewType": "XML",
					"viewName": "PageNotFound",
					"title": "NotFound",
					"viewId": "NotFound",
					"viewLevel": 5,
					"controlAggregation": "pages",
					"controlId": "mAppId"
				}
			}
		}
	}
}